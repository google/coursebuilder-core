#: experimental/coursebuilder/modules/invitation/invitation.py:287
#, python-format
msgid "Error: You have already sent an invitation email to \"%s\""
msgstr "Virhe: olet jo lähettänyt sähköpostikutsun seuraavaan osoitteeseen: %s"

#. Error indicating not all email messages were sent.
#: experimental/coursebuilder/modules/invitation/invitation.py:304
#, python-format
msgid "Not all messages were sent (%s / %s):"
msgstr "Kaikkia viestejä ei lähetetty (%s / %s):"

#. Success message indicating number of emails sent.
#: experimental/coursebuilder/modules/invitation/invitation.py:312
#, python-format
msgid "OK, %s messages sent"
msgstr "OK, %s viestiä lähetettiin"

#. Title encouraging user to invite friends to join a course
