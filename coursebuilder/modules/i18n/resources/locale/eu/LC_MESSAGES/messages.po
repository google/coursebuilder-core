#: experimental/coursebuilder/modules/invitation/invitation.py:287
#, python-format
msgid "Error: You have already sent an invitation email to \"%s\""
msgstr "Errorea: ez duzu gonbidapenik bidali \"%s\" helbide elektronikora"

#. Error indicating not all email messages were sent.
#: experimental/coursebuilder/modules/invitation/invitation.py:304
#, python-format
msgid "Not all messages were sent (%s / %s):"
msgstr "Ez dira mezu guztiak bidali (%s / %s):"

#. Success message indicating number of emails sent.
#: experimental/coursebuilder/modules/invitation/invitation.py:312
#, python-format
msgid "OK, %s messages sent"
msgstr "Ados, %s mezu bidali dira"

#. Title encouraging user to invite friends to join a course
#: experimental/coursebuilder/modules/invitation/invitation.py:365
msgid "Invite Friends"
msgstr "Gonbidatu lagunak"

#. Label on control asking user to invite friends to join.
#: experimental/coursebuilder/modules/invitation/invitation.py:370
msgid "Click to send invitations to family and friends"
msgstr "Egin klik lagun eta senideak gonbidatzeko"

#. Inviting friends to join a course is not currently enabled.
#: experimental/coursebuilder/modules/invitation/invitation.py:374
msgid "Invitations not currently available"
msgstr "Une honetan ez dago gonbidapenik erabilgarri"

#. Control allowing user to subscribe/unsubscribe from email invitation
#: experimental/coursebuilder/modules/invitation/invitation.py:386
msgid "Subscribe/Unsubscribe"
msgstr "Harpidetu edo kendu harpidetza"

