msgstr " Enregistrer un brouillon "

#. Text on the button for submitting a peer review.
#: experimental/coursebuilder/views/assessment_html_content.html:35
#: experimental/coursebuilder/views/base.html:47
msgid " Submit Review "
msgstr " Envoyer la correction "

#. Text on the button for checking a set of answers.
#: experimental/coursebuilder/views/assessment_html_content.html:43
#: experimental/coursebuilder/views/base.html:23
