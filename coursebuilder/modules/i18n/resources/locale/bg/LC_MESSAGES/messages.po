msgstr " Изпращане на проверката "

#. Text on the button for checking a set of answers.
#: experimental/coursebuilder/views/assessment_html_content.html:43
#: experimental/coursebuilder/views/base.html:23
msgid " Check Answers "
msgstr " Проверка на отговорите "

#. Text on the button for submitting answers.
#: experimental/coursebuilder/views/assessment_html_content.html:52
#: experimental/coursebuilder/views/base.html:39
msgid " Submit Answers "
msgstr " Изпращане на отговорите "

#. Title of the webpage.
#: experimental/coursebuilder/views/assessment_page.html:15
msgid "Assessment"
msgstr "Оценяване"

#. Text on the button for showing the answer to a question.
#: experimental/coursebuilder/views/base.html:27
